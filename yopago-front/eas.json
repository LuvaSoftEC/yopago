{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64",
        "ORG_GRADLE_JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64",
        "EXPO_PUBLIC_BASE_URL": "http://localhost:8080/api",
        "EXPO_PUBLIC_ISSUER": "http://localhost:8082/realms/yopago"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BASE_URL": "http://localhost:8080/api",
        "EXPO_PUBLIC_ISSUER": "http://localhost:8082/realms/yopago"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
